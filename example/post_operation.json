{
    "type": "POST",
    "name": "Add Quarterback",
    "description": "Adds an NFL quarterback to a dataset",
    "url": "http://127.0.0.1:5000",
    "path": "/add",
    "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
            "body": {
                "type": "object",
                "properties": {
                    "qb": {
                        "type": "string",
                        "description": "The name of the quarterback"
                    }
                },
                "required": [
                    "qb"
                ]
            }
        },
        "required": [
            "body"
        ]
    }
}